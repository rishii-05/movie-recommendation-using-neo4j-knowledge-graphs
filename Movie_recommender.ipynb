    "# --- Load Data into Neo4j ---\n",
    "print(\"Starting to load data into Neo4j. This may take a few minutes...\")\n",
    "\n",
    "def run_cypher_query(query, params={}):\n",
    "    \"\"\"A helper function to run queries with the Neo4j driver\"\"\"\n",
    "    with driver.session() as session:\n",
    "        session.run(query, params)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 18,
   "id": "d45d41e4",
   "metadata": {},
   "outputs": [],
   "source": [
    "# 1. Create Constraints (for speed and to prevent duplicates)\n",
    "# This tells Neo4j that 'title' on a Movie, 'name' on an Actor, etc., must be unique.\n",
    "run_cypher_query(\"CREATE CONSTRAINT IF NOT EXISTS FOR (m:Movie) REQUIRE m.title IS UNIQUE\")\n",
    "run_cypher_query(\"CREATE CONSTRAINT IF NOT EXISTS FOR (a:Actor) REQUIRE a.name IS UNIQUE\")\n",
    "run_cypher_query(\"CREATE CONSTRAINT IF NOT EXISTS FOR (d:Director) REQUIRE d.name IS UNIQUE\")\n",
    "run_cypher_query(\"CREATE CONSTRAINT IF NOT EXISTS FOR (g:Genre) REQUIRE g.name IS UNIQUE\")"
   ]
  },
